
body {font-family: Consolas; font-size:22px; margin: 0;}
.all {border: 1px solid gray; width:100%; 
	background-image: url('file:///D://Tools//web.py-0.33//dianyi_//static//images//background.png');
	background-repeat: repeat}
.base_head {margin: 0 auto; width:900px; border: 2px solid gray}
.base_container {margin: 0 auto; width:900px; height:750px; border: 2px solid gray}

#con_left {float: left; height: 750px; width: 640px; border: 1px solid gray; background-color: #D3D3D3}
#con_right {float: right; height: 750px; width: 250px; border: 1px solid gray; background-color: #D3D3D3;
		font-size: 16px; }

#con_right h3 {
	height: 25px; 
	padding: 4px 0px 0px 13px; 
	background: none repeat scroll 0% 0% rgb(29, 29, 29); 
	font: 16px;
}

dd{ 
	display: none;
	border: 1px solid gray;
}
dl dt {
    font: 14px;
    background: #DFG;
    border: 1px solid gray;
}


.base_tail {margin: 0 auto; width:100%; height:100px; border: 2px solid gray; background-color: #B0E0E6}

#logo {height: 100px;width: 180px; border: 1px solid gray}
.navigation {height:46px;width: 900px; border: 1px solid gray}
#consult {height: 30px;width: 900px; border: 1px solid gray; background-color: black;}



.all #box { width:900px; height:300px;
	border-radius:1px;border:1px solid gray; background-color: gray}
#box .list{position:relative;width:850px;height:270px;overflow:hidden;border:1px solid #ccc;}
#box .list li{position:absolute;top:0;left:0;width:900px;height:200px;opacity:0;filter:alpha(opacity=0);}
#box .list li.current{opacity:1;filter:alpha(opacity=100);}

#box .count {position:absolute;right:0;bottom:5px;}
#box .count li{color:#fff;float:left;width:20px;height:20px;cursor:pointer;margin-right:5px;overflow:hidden;background:#F90;opacity:0.7;filter:alpha(opacity=70);border-radius:20px;}
#box .count li.current{color:#fff;opacity:1;filter:alpha(opacity=100);font-weight:700;background:#f60;}
		/*#tmp{width:100px;height:100px;background:red;position:absolute;}*/

/*导航栏*/
#menu {height:46px;
	background:url(file:///D://Tools//web.py-0.33//dianyi_//static//images//nav_bg.jpg) 0 0 repeat-x;}
#menu ul{ padding:0;border:0;list-style:none;}

#nav {padding:0;border:0;list-style:none;display:block; font-size:14px;}

#nav .mainlevel {background:url(file:///D://Tools//web.py-0.33//dianyi_//static//images//nav_bg.jpg) 0 0 repeat-x;height:46px;float:left;/*IE6 only*/}
#nav .mainlevel a {color:#fff; text-decoration:none; line-height:46px; display:block; text-align:center;width:108px;}
#nav .mainlevel a:hover {color:#fff; text-decoration:none; background:url(file:///D://Tools//web.py-0.33//dianyi_//static//images//nav_hover.jpg) no-repeat; width:108px;}

#nav .mainlevel ul {display:none; position:absolute;float: left;border:#3c4956 1px solid;border-top:none;font-size:16px;}
#nav .mainlevel li { width:108px;/*IE6 only*/}
#nav .mainlevel li a{background:#a8b2ba;color:#000; }
#nav .mainlevel li a:hover{background:#3a4754; color:#fff;}

/*课程列表栏*/
#spe_title {
	height: 50px;
	border: 1px solid gray
}
#spe_title_ok {
	float: left;
	height: 18px;
	width: 20%;
	font-size: 14px;
	border: 1px solid gray
}
ul.spe_nav {
	float: right;
	height: 40px;
	width: 80%;
	list-style: none;
	display: inline;
	border: 1px solid gray;
	text-align: center;
	background-color: gray;
}
ul.spe_nav li {
	display: inline-table;
	padding: 5px 1px;
	line-height: 16px;
}
ul.spe_nav h2 {
	font-size: 12px;
	height: 12px;
}

.test ul{height:30px; width:600px; list-style:none; font-size: 12px;border: 1px solid black} 
.test li{float:left;width:80px;background:#CCC;margin-left:3px;line-height:30px;} 
.test a{display:block;text-align:center;height:30px;} 
.test a:link{color:#666;text-decoration:none;} 
.test a:visited{color:#666;text-decoration:underline;} 
.test a:hover{color:#FFF;text-decoration:none;background:url(arrow_on.gif) #F00 no-repeat 5px 12px;}  

/*主显示框*/
#con_left #spe_display {
	height: 300px;
	width: 640px;
	background-color: #CCC;
	border: 1px solid red;
	margin-bottom: 10px;
}
/*主显示框 内容 6个小版块*/
#spe_display .spe_display_content {
	display: none;
	height: 300px;
	width: 640px;
}

#spe_display #spe_display_1 {
	display: block;
}

/*图片循环播放*/
#pic_for {
	height: 200px;
	width: 640px;
	background-color: #CCC;
	border: 1px solid red;
}





