{% extends 'base.htm' %}
{% load filters %}
{% block title %}
    广州点艺设计培训-室内|环艺|建筑|动画|培训学校  高端数字视觉教育品牌
{% endblock %}
$.bootstrapIE6(el)
{% block content %}
        <div class="container">
            <!-- Main hero unit for a primary marketing message or call to action -->
            <!-- <div> -->
                <div id="myCarousel" class="carousel">
                    <!-- Carousel items -->
                    <div class="carousel-inner" style="border-radius: 4px;">
                        {% for ad_pic in ad_pic_list %}
                            {% if forloop.first %}
                            <div class="item active">
                                <a href="/dyhome/focus/{{ad_pic.id}}" title="{{ad_pic.title}}">
                                <img class="" src="{{ ad_pic.image }}" alt="#" style="height:320px;"/>
                                <div class="carousel-caption" style="height:10px;">
                                    <h4 class="pull-right" style="margin-top:-10px;">{{ ad_pic.sub_title }}</h4>
                                </div>
                                </a>
                            </div>
                            {% else %}
                            <div class="item">
                                <a href="/dyhome/focus/{{ad_pic.id}}" title="{{ad_pic.title}}">
                                <img src="{{ ad_pic.image }}" alt="#" style="height:320px;"/>
                                <div class="carousel-caption" style="height:10px;">
                                    <h4>{{ ad_pic.sub_title }}</h4>
                                </div>
                                </a>
                            </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                    <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
                    <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
                </div>
            <!-- </div> -->
        </div><!-- Carousel nav -->
        <div class="container">
            <div class="row">
                <div id="span4" class="span4">
                    <h4 class="h4_title"> <a class="pull-right span_more label" href="/dyhome/news/1" title="更多新闻等着你">更多</a>点艺新闻</h4>
                    <ul class="unstyled">
                        <!-- span></span> -->
                        {% for news in dy_news_list %}
                            <span class="span-right" style="font-size:12px;"> {{ news.publication_date|date:"m/d" }} </span>
                            <li class="li_list" title="{{news.title}}"> <img src="/static/images/index/dot.jpg" class="img_dot"> <a href="/dyhome/news_display/{{ news.id }}">{{ news.title|dytruncate:"19" }}</a> </li>
                        {% endfor %}
                    </ul>
                </div>
                <div id="span5" class="span5">
                    <h4 class="h4_title"> <a class="pull-right span_more label" href="/dyhome/course/0/0/" title="更多教程等着你">更多</a> 最新教程 </h4>
                    <ul class="unstyled">
                        {% for course in course_news_list %}
                            <span class="span-right" style="font-size:12px;"> [{{ course.course_category }}]</span>
                            <li class="li_list" title="{{course.title}}"> <img src="/static/images/index/dot.jpg" class="img_dot"> <a href="/dyhome/course_display/{{ course.id }}">{{ course.title|dytruncate:"28" }}</a></li>
                        {% endfor %}
                    </ul>
                </div>
                <div id="span3" class="span3" >
                    <h4 class="h4_title">开班信息</h4>
                    <ul class="unstyled">
                        {% for class_ in class_list %}
                            <span class="span-right" style="; color:#8DB6CD; height:20px;"> <strong> {{ class_.publication_date|date:"m月d日" }}</strong></span>
                            <li class="li_list" title="{{class_.title}}"> <img src="/static/images/index/dot.jpg" class="img_dot"> <a href="/dyhome/class/{{class_.id}}/">{{ class_.title|dytruncate:"12" }}</a></li>
                        {% endfor %}
                    </ul>
                </div>
            </div>            
        </div>

        <!-- new nav -->
    <div class="container">
        <div class="row">
            <div id="span9" class="span9">
                <div class="tabbable" >

                  <ul class="nav nav-tabs" style="border-bottom: 1px solid gray;margin-bottom: 2px;  border-radius: 4px;">
                        <li> <!-- <h4 class="" style="height:28px; padding-top:10px;;"><strong>学生作品</strong>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</h4> --> <h4 style=""> <strong> 学生作品 </strong>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</h4> </li>
                        {% for course in course_list %}
                            {% if forloop.first %}
                                <li class="active"> <a href="#{{course.id}}" data-toggle="tab"> {{course.name}} </a></li>
                            {% else %}
                                <li> <a href="#{{course.id}}" data-toggle="tab"> {{course.name}} </a></li>
                            {% endif %}
                        {% endfor %}
                    </ul>
                    <div class="tab-content row">
                        {% for opus in opus_list %}
                            {% if forloop.first %}
                                <div class="tab-pane active" id="{{forloop.counter}}">
                                    <ul class="thumbnails">
                                    {% for op in opus %}
                                        {% if forloop.first %}
                                            <li class="span4 " style="margin-left: 0px; padding-left: 40px;" id="span4">
                                                <a href="#" class="thumbnail">
                                                  <img alt="" src="{{op.image}}" style="height:298px;width:320px;float:left;"><span style="color:gray;">{{op.title}}&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp{{op.author}}</span>
                                                </a>
                                            </li>
                                        {% else %}
                                            <li class="span2-5" style="margin-left:10px; margin-bottom:10px;" id="span2-5">
                                                <a href="#" class="thumbnail">
                                                  <img data-src="holder.js/260x180" alt="" src="{{op.image}}" style="height:128px;width:192px;float:left;"><span style="color:gray;">{{op.title}}&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp{{op.author}}</span>
                                                </a>
                                            </li>
                                        {% endif %}
                                    {% endfor %}
                                    </ul>
                                </div>
                            {% else %}
                                <div class="tab-pane" id="{{forloop.counter}}">
                                    <ul class="thumbnails">
                                    {% for op in opus %}
                                        {% if forloop.first %}
                                            <li class="span4 " style="margin-left: 0px; padding-left: 40px;" id="span4">
                                                <a href="#" class="thumbnail">
                                                  <img alt="" src="{{op.image}}" style="height:300px;width:320px;float:left;"><span style="color:gray;">{{op.title}}&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp{{op.author}}</span>
                                                </a>
                                            </li>
                                        {% else %}
                                            <li class="span2-5" style="margin-left:10px; margin-bottom:10px;" id="span2-5">
                                                <a href="#" class="thumbnail">
                                                  <img data-src="holder.js/260x180" alt="" src="{{op.image}}" style="height:128px;width:192px;float:left;"><span style="color:gray;">{{op.title}}&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp{{op.author}}</span>
                                                </a>
                                            </li>
                                        {% endif %}
                                    {% endfor %}
                                    </ul>
                                </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>
            </div>
            <div class="span3" id="span3" style="font-size: 12px;">
                <h4 class="h4_title">就业名单</h4>
                <table class="table table-striped table-borde#F7F7F7">
                    <thead>
                      <tr>
                        <th>班级</th>
                        <th>姓名</th>
                        <th>就业公司</th>
                      </tr>
                    </thead>
                        {% for job in job_list %}
                            <tr>
                            <td title="{{job.course_class.title}}">{{ job.course_class.title|dytruncate:"7" }}</td>
                            <td title="{{job.name.name}}">{{ job.name.name|dytruncate:"3" }}</td>
                            <td title="{{job.company}}">{{ job.company|dytruncate:"5" }}</td>
                            </tr>
                        {% endfor %}
                    </tbody>
                  </table>
                  <!-- <hr/> -->
                <span class="span2" id="span2" style="margin:0px;">
                  <img src="/static/images/index/weixin.jpg" style="width:115px; height: 115px;">
                </span>
                <div class="span1" id="span1" style="margin: 10px 0 0 0;">
                    <span> 在线咨询 </span> <br style="height:10px;" />
                  <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=1902935253&site=qq&menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=2:1902935253:41 &r=0.008532069833831312" alt="小毛老师" title="小毛老师"></a>
                  <span> -------------- </span>
                  <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=2234484590&site=qq&menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=2:2234484590:41 &r=0.7360129433758769" alt="小周老师" title="小周老师"></a>
              </div>


                <!-- </span>
                <span>
                  <a href="#" title="豆瓣小组"><img src="/static/images/index/weixin.jpg" style="width:115px; height: 115px;"></a>
                  
                </span>
                <span>
                  <a href="#" title="人人小组"><img src="/static/images/index/weixin.jpg" style="width:115px; height: 115px;"></a>
                  
                </span> -->
            </div>
        </div>
    </div>
{% endblock %}

{% block footer %}
    {% include 'footer.htm' %}
{% endblock %}
